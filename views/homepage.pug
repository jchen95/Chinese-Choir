extends layout

block content
    // Page Content
    .container
      // Heading Row
      .row.my-4
        .col-lg-12
          #demo.carousel.slide(data-ride='carousel')
            ul.carousel-indicators
              li.active(data-target='#demo', data-slide-to='0')
              li(data-target='#demo', data-slide-to='1')
              li(data-target='#demo', data-slide-to='2')
            .carousel-inner
              .carousel-item.active
                img(src='http://www.sjsu.edu/music/pics/sjsu_choir_02.jpg', alt='Los Angeles', width='1100', height='400')
                .carousel-caption
                  h3 Los Angeles
                  p We had such a great time in LA!
              .carousel-item
                img(src='https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcRezEGbTNIUCOkpo9a_pHuthIY6gllnw_qLGIGbu8iNVjCqslGr', alt='Chicago', width='1100', height='400')
                .carousel-caption
                  h3 Chicago
                  p Thank you, Chicago!
              .carousel-item
                img(src='https://www.mormontabernaclechoir.org/content/dam/motab/blog/2017/09/choir-tour-2018-blog.jpg/_jcr_content/renditions/cq5dam.web.1280.1280.jpeg', alt='New York', width='1100', height='400')
                .carousel-caption
                  h3 New York
                  p We love the Big Apple!
            a.carousel-control-prev(href='#demo', data-slide='prev')
              span.carousel-control-prev-icon
            a.carousel-control-next(href='#demo', data-slide='next')
              span.carousel-control-next-icon
